{
  "manifest_version": 2,

  "name": "Epiphany",
  "description": "Share if you care",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },

  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://epip.nl/"
    "tabs"
  ]
}
